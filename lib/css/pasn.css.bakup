/* SURVEY */
.alterselectsubmit, .networkalterselectsubmit {
	width: 50%; 
	background-color: #F7F8FC;
	color: #333;
	text-align: center;
	border: 2px solid #709CCB;
	width: 500px ;
    margin-left: auto ;
    margin-right: auto ;

}
.alterselectsubmit_hover, .networkalterselectsubmit_hover {
	border: 2px solid #507AA0;
	background-color: #D5D6DA;
}

.alterBucketBox{
    width: 80%;
    list-style-type: none;
}
.alterBucket {
	border: 1px solid #83B87C;
    background-color: #A7C5BD;
   	width: 100%;
	padding: 5px;
    margin  : 5px;
	display: block;
	text-align: right;
    text-decoration: none;
    color: #222;
}
.alterBucketBox_activeheader {
	background-color: #C8F5E9;
	font-weight: bold;
}

.alterBucketBox div {
	float: left; 
	height: 300px;
}

.alter{
	float: left;
	width: 70px;
   	height: 70px;
 	margin: 2px;
   	padding: 4px;
	background-color: #709CCB; 
	border: 2px solid #000;    	
/*	font-weight: bold; */
	font-size: 10px;	
	text-align: center;
	color: #eee;
	overflow: hidden;
	text-overflow: ellipsis;
}
