#message {
	width: 90%;
	height: 40px;
	padding: 3px;
	margin: 4px; 
	background-color: #F1FFCF;
	border: 2px #FFF;
	text-align: center;	
}

#leftcontent {
	width: 66%;
	float: left;
}

#sidebar {
	width: 33%;
	float: left;
	position: absolute;
	top: 100;
	right: 10;	
}


/* INTERNAL FRIEND MANAGER */

#internalFriendManagerHeader {
	width: 100%;
	float; left;
	top: 5;
	}

#bucket_list {
	list-style-type: none;	
}
.bucket_label {
	float: left;	
	height: 100px;
	width: 100px;
	background-color: #AAA;
	padding: 3px;
	margin: 4px;
	border: 2px solid #000;
	font-weight: bold;
	text-align: center;
}

.bucket_label_hover {
/*	border: 3px solid #039; */
background-color: #FFF;	
}

#new_friend_list{
	list-style-type: none;
}
.new_friend_entry {
/*	height: 70px;
	background-color: #709CCB;
	padding: 3px;
	margin: 4px;
	
	font-weight: bold;
	text-align: center;
	color: #eee; */
	float: left;
	width: 70px;
